<h1>立法會會議提案表決紀錄</h1>

<table id="motion_list" class="table table-striped table-bordered table-responsive table-hover" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>日期</th>
            <th>動議</th>
            <th>動議人</th>
          </tr>
        </thead>
        <tbody>
          <% @result.each do |item| %>
            <tr data-id='<%=item['id']%>' class="modal-trigger" onclick="var item= '<%=item['id']%>';analyze(item);" data-toggle="modal" data-target="#myModal">
              <td data-id='<%=item['id']%>'><%= item["date"] %></td>
              <td data-id='<%=item['id']%>'><%= item[:motion_ch] %></td>
              <td data-id='<%=item['id']%>'><%= item[:mover_ch] %></td>
            </tr>
          <% end %>
        </tbody>
    </table>